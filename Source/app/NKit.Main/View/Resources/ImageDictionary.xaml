  <ControlTemplate x:Key="Image_Arrow-Up">
    <Rectangle RenderTransformOrigin="0.5,0.5" Fill="{StaticResource Image_ArrowBase}">
      <Rectangle.RenderTransform>
        <TransformGroup>
          <ScaleTransform ScaleX="1" ScaleY="-1"/>
        </TransformGroup>
      </Rectangle.RenderTransform>
    </Rectangle>
  </ControlTemplate>
  <ControlTemplate x:Key="Image_Arrow-Down">
    <Rectangle RenderTransformOrigin="0.5,0.5" Fill="{StaticResource Image_ArrowBase}">
      <Rectangle.RenderTransform>
        <TransformGroup>
          <RotateTransform Angle="0"/>
        </TransformGroup>
      </Rectangle.RenderTransform>
    </Rectangle>
  </ControlTemplate>
  <ControlTemplate x:Key="Image_Arrow-Left">
    <Rectangle RenderTransformOrigin="0.5,0.5" Fill="{StaticResource Image_ArrowBase}">
      <Rectangle.RenderTransform>
        <TransformGroup>
          <RotateTransform Angle="90" />
        </TransformGroup>
      </Rectangle.RenderTransform>
    </Rectangle>
  </ControlTemplate>
  <ControlTemplate x:Key="Image_Arrow-Right">
    <Rectangle RenderTransformOrigin="0.5,0.5" Fill="{StaticResource Image_ArrowBase}">
      <Rectangle.RenderTransform>
        <TransformGroup>
          <ScaleTransform ScaleX="-1" ScaleY="1"/>
          <RotateTransform Angle="-90" />
        </TransformGroup>
      </Rectangle.RenderTransform>
    </Rectangle>
  </ControlTemplate>

